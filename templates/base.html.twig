<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}{{ "Page d'accueil" | trans }}{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>
        <ul>
            <li><a href="{{ path('page_index') }} ">{{ "Pages" | trans }}</a></li>
            <li><a href="{{ path('post_index') }} ">{{ "Publications" | trans }}</a></li>
            <li><a href="{{ path('comment_index') }} ">{{ "Commentaires" | trans }}</a></li>
            <li><a href="{{ path('user_index') }} ">{{ "Membres" | trans }}</a></li>
        </ul>
        {% block body %}{% endblock %}
    </body>
</html>
